<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <title><%- @document.title %></title>

    <meta name="description" content="<%- @document.description %>">
    <meta name="keywords" content="Jimmy, King, Jimmy King, web developer, front-end, developer, software, web app, Tempe, Phoenix, Mesa, Scottsdale, AZ, Arizona, JavaScript, jQuery, Node.js, Sass, SCSS, LESS, HTML, CSS, LESS, CSS3, HTML5, XSLT, Scrum, Agile, Git, <%- @document.keywords %>">
    <meta http-equiv="content-language" content="en-us" />
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="/css/styles.css">
    <% if @document.css : %>
    <% for style in @document.css : %>
      <link rel="stylesheet" href="<%= style %>">
    <% end %>
    <% end %>

    <% if @document.typekit : %>
      <script type="text/javascript" src="http://use.typekit.com/jwk1aqk.js"></script>
      <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
    <% end %>
  </head>
  <body class="<%- @document.body_class %>">
    <%- @partial 'nav.html.coffee', @ %>

    <div class="content <%- @document.content_class %>">
      <%- @content %>
    </div>
    
    <% if @document.js : %>
    <% for script in @document.js: %>
      <script src="<%= script %>"></script>
    <% end %>
    <% end %>
    
    <script>
      var _gauges = _gauges || [];
      (function() {
        var t   = document.createElement('script');
        t.type  = 'text/javascript';
        t.async = true;
        t.id    = 'gauges-tracker';
        t.setAttribute('data-site-id', '50991a3af5a1f560e1000001');
        t.src = '//secure.gaug.es/track.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(t, s);
      })();
    </script>
  </body>
</html>
